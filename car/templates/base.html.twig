<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %} Car Demo {% endblock %}</title>
        {% block stylesheets %}
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">         
        {% endblock %}
    </head>
    <body>
        {% block nav %}
            <ul class="nav justify-content-center">
                <li class="nav-item">
                    <h1><a class="nav-link text-danger" href="{{ path("home") }}">HOME</a></h1>
                </li>
                <li class="nav-item">
                    <h1><a class="nav-link" href="{{ path("car_index") }}">Car</a></h1>
                </li>
                <li class="nav-item">
                    <h1><a class="nav-link" href="{{ path("customer_index") }}">Customer</a></h1>
                </li>
                <li class="nav-item">
                    <h1><a class="nav-link" href="{{ path("part_index") }}">Part</a></h1>
                </li>
                <li class="nav-item">
                    <h1><a class="nav-link" href="{{ path("sale_index") }}">Sale</a></h1>
                </li>
                <li class="nav-item">
                    <h1><a class="nav-link" href="{{ path("supplier_index") }}">Supplier</a></h1>
                </li>
            </ul>
        {% endblock %}
        
        {% block body %}
        
        {% endblock %}
    </body>
</html>
